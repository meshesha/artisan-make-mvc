            "Meshesha\\ArtisanMakeMvc\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "meshesha",
            "email": "tadytiodros@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Meshesha\\ArtisanMakeMvc\\ArtisanMakeMvcServiceProvider"
            ]
        }
