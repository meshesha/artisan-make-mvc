{
    "name": "meshesha/artisan-make-mvc",
    "description": "Laravel artisan command to create view, controller and route from existing Model",
    "license": "MIT",
    "keywords": [
        "laravel",
        "artisan",
        "laravel-package",
        "package",
        "artisan",
        "artisan-command",
        "laravel-views",
        "laravel-model",
        "laravel-controller",
        "laravel-route",
        "laravel-mvc",
        "laravel-crud"
    ],
    "autoload": {
